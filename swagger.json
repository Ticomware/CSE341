    "/contacts/": {
      "get": {
        "description": "Returns all contacts in the database.",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "description": "Stores a contact in the database.",
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "description": "Adds a contact",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "John"
                },
                "lastName": {
                  "type": "string",
                  "example": "Doe"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@example.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "Blue"
                },
                "birthday": {
                  "type": "string",
                  "example": "01/01/00"
                }
              },
              "required": [
                "firstName",
                "lastName",
                "email",
                "favoriteColor",
                "birthday"
              ]
            }
          }
        ],
        "responses": {}
      }
    },
    "/contacts/{id}": {
      "get": {
        "description": "Returns one contact from the database.",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "put": {
        "description": "Updates a contact in the database.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "The ID for the contact to update"
          },
          {
            "name": "obj",
            "in": "body",
            "description": "Updates a contact",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "John"
                },
                "lastName": {
                  "type": "string",
                  "example": "Doe"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@example.com"
                },
                "favoriteColor": {
                  "type": "string",
                  "example": "Blue"
                },
                "birthday": {
                  "type": "string",
                  "example": "01/01/00"
                }
              },
              "required": [
                "firstName",
                "lastName",
                "email",
                "favoriteColor",
                "birthday"
              ]
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      },
      "delete": {
        "description": "Deletes a contact from the database.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "The ID for the contact to delete"
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
